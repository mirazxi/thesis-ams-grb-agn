    "from pathlib import Path\n",
    "\n",
    "BASE = Path.home() / \"thesis\"\n",
    "DATA = BASE / \"data\" / \"processed\"\n",
    "OUT  = BASE / \"outputs\"\n",
    "OUT_TABLES = OUT / \"tables\"\n",
    "OUT_TEX    = OUT / \"tex\"\n",
    "\n",
    "for p in [OUT_TABLES, OUT_TEX]:\n",
    "    p.mkdir(parents=True, exist_ok=True)\n",
    "\n",
    "print(\"DATA:\", DATA)\n",
    "print(\"OUT :\", OUT)\n"
   ]
